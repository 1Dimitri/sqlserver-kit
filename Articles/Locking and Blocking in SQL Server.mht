FROM sys.configurations
WHERE value &lt;&gt; value_in_use;
GO

exec sp_configure 'show advanced options', 1;
GO
RECONFIGURE
GO

exec sp_configure 'blocked process threshold (s)', 20;
GO
RECONFIGURE
GO
